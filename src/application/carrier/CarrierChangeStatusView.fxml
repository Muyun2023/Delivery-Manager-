<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="300.0" minWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.carrier.CarrierChangeStatusViewController">
   <children>
      <ImageView fitHeight="427.0" fitWidth="679.0" layoutX="-79.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/backgrounds/WechatIMG1074.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="172.0" fitWidth="537.0" layoutX="73.0" layoutY="169.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/packagestatus.png" />
         </image>
      </ImageView>
      <Button fx:id="btnHome" layoutX="26.0" layoutY="25.0" mnemonicParsing="false" onAction="#btnHomeClick" text="Home" />
      <Button fx:id="btnReturn" layoutX="91.0" layoutY="26.0" mnemonicParsing="false" onAction="#btnReturnClick" text="Return" />
      <ChoiceBox fx:id="choiceStatus" layoutX="279.0" layoutY="132.0" prefHeight="25.0" prefWidth="172.0" />
      <TextField fx:id="trackNumInput" layoutX="73.0" layoutY="132.0" prefHeight="27.0" prefWidth="199.0" promptText="Please Enter Track Number" />
      <Label layoutX="190.0" layoutY="78.0" text="Set Package Status" textFill="#cd7116">
         <font>
            <Font name="Arial Bold Italic" size="23.0" />
         </font>
      </Label>
      <Button fx:id="btnConfirm" layoutX="458.0" layoutY="133.0" mnemonicParsing="false" onAction="#btnConfirmClick" text="Confirm" />
      <Label fx:id="statusInfo" layoutX="73.0" layoutY="106.0" prefHeight="25.0" prefWidth="447.0" textFill="#1761b5">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
