<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="300.0" minWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.recipient.RecipientMainViewController">
   <children>
      <ImageView fitHeight="336.0" fitWidth="620.0" layoutX="1.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/recipient/RecipientMain.png" />
         </image>
      </ImageView>
      <Button fx:id="btnStatus" layoutX="340.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnStatusClick" prefHeight="23.0" prefWidth="156.0" text="Package Status" />
      <Button fx:id="btnConfirm" layoutX="340.0" layoutY="132.0" mnemonicParsing="false" onAction="#btnConfirmClick" prefHeight="23.0" prefWidth="156.0" text="Confirm Reciept &amp; Help" />
      <Button fx:id="btnPreference" layoutX="340.0" layoutY="184.0" mnemonicParsing="false" onAction="#btnPreferenceClick" prefHeight="23.0" prefWidth="156.0" text="Notification Preference" />
      <Button fx:id="btnRate" layoutX="340.0" layoutY="232.0" mnemonicParsing="false" onAction="#btnRateClick" prefHeight="23.0" prefWidth="156.0" text="Rate Us" />
      <Button fx:id="btnHome" layoutX="23.0" layoutY="22.0" mnemonicParsing="false" onAction="#btnHomeClick" text="Home" />
      <Button fx:id="btnReturn" layoutX="91.0" layoutY="23.0" mnemonicParsing="false" onAction="#btnReturnClick" text="Return" />
   </children>
</AnchorPane>
