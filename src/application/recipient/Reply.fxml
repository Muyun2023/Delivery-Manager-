<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="300.0" minWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.recipient.ReplyController">
   <children>
      <ImageView fitHeight="340.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/recipient/Reply.png" />
         </image>
      </ImageView>
      <Label layoutX="217.0" layoutY="64.0" prefHeight="33.0" prefWidth="342.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextArea layoutX="233.0" layoutY="156.0" prefHeight="108.0" prefWidth="207.0" promptText="Any Problem please enter Here! We will contact you ASAP!" />
      <CheckBox fx:id="btnConfirm" layoutX="233.0" layoutY="117.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="330.0" text="Confirm reciept ! The notification will be cancelled." />
      <Button fx:id="btnSubmit" layoutX="450.0" layoutY="156.0" mnemonicParsing="false" onAction="#btnSubmitClick" prefHeight="17.0" prefWidth="97.0" text="Submit" />
      <Button fx:id="btnReturn" layoutX="122.0" layoutY="30.0" mnemonicParsing="false" onAction="#btnReturnClick" text="Return" />
      <Label fx:id="myMessage" layoutX="450.0" layoutY="186.0" prefHeight="23.0" prefWidth="97.0" />
      <Button fx:id="btnLoginOut" layoutX="34.0" layoutY="30.0" mnemonicParsing="false" onAction="#btnLoginOutClick" text="Log Out" />
   </children>
</AnchorPane>
